{
  "name": "privacy-voting-example",
  "version": "1.0.0",
  "description": "Privacy-preserving delegated voting system using FHEVM SDK - Example integration",
  "main": "index.html",
  "scripts": {
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:localhost": "hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "verify:sepolia": "hardhat run scripts/verify.js --network sepolia",
    "interact": "hardhat run scripts/interact.js",
    "interact:localhost": "hardhat run scripts/interact.js --network localhost",
    "interact:sepolia": "hardhat run scripts/interact.js --network sepolia",
    "simulate": "hardhat run scripts/simulate.js --network localhost"
  },
  "keywords": [
    "fhevm",
    "sdk",
    "privacy",
    "voting",
    "blockchain",
    "encryption",
    "delegation",
    "ethereum"
  ],
  "author": "FHEVM Contributors",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.2",
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.3",
    "hardhat": "^2.26.4"
  },
  "dependencies": {
    "@fhevm/sdk": "workspace:*"
  }
}
